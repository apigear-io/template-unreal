{
  "name": "@apigear-io/epic-blueprint",
  "version": "1.15.4",
  "private": true,
  "description": "An unreal blueprint code generator",
  "keywords": [
    "objectapi",
    "epic",
    "unreal",
    "blueprint",
    "c++",
    "library",
    "client"
  ],
  "devDependencies": {
    "@apigear-io/apigear-cli": "^0.4.0",
    "standard-version": "^9.5.0"
  },
  "scripts": {
    "gen:goldenmaster": "rm -rf goldenmaster && node node_modules/@apigear-io/apigear-cli/bin/run gen run apigear/goldenmaster.solution.yaml && mv test/ goldenmaster",
    "test:goldenmaster": "rm -rf test && node node_modules/@apigear-io/apigear-cli/bin/run gen run apigear/goldenmaster.solution.yaml && diff -r goldenmaster/ test/",
    "report:license": "license-report --exclude=@apigear-io/apigear-cli --department.value=apigear --relatedTo.value=$npm_package_name --output=csv --csvHeaders > licenses.csv",
    "version": "standard-version -i CHANGELOG.md --same-file"
  },
  "author": {
    "name": "ApiGear UG",
    "url": "https://github.com/apigear-io/template-unreal"
  },
  "homepage": "https://github.com/apigear-io/objectapi-generator#readme",
  "license": "SEE LICENSE IN LICENSE.TXT",
  "files": [
    "/templates",
    "/rules.yaml",
    "/README.md",
    "/LICENSE"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apigear-io/template-unreal.git"
  },
  "bugs": {
    "url": "https://github.com/apigear-io/template-unreal/issues"
  }
}
