#include "{{module.name|capital}}_data.h"

{% for enum in module.enums %}
{% assign class = enum.name %}
/**
 * Enumeration {{enum|unrealTypeName: false, moduleCapital}}
 */
bool toUE4Type({{enum|unrealTypeName: false, moduleCapital}}& value, uint8 v)
{
    bool bSuccessful = false;
    switch (v) {
{% for member in enum.members %}
        case {{member.value}}:
            value = {{enum|unrealTypeName: false, moduleCapital}}::{{member.name|constant}};
            bSuccessful = true;
            break;
{% endfor %}
        default:
            value = {{enum|unrealDefault: moduleCapital}};
    }
    return bSuccessful;
}

{% endfor %}

{% for struct in module.structs %}
bool {{struct|unrealTypeName: false, moduleCapital}}::operator==(const {{struct|unrealTypeName: false, moduleCapital}}& rhs) const
{
	return
{% for field in struct.fields %}
	{% unless forloop.first %}&& {% endunless %}({{field.name}} == rhs.{{field.name}}){% if forloop.last %};{% endif %}
{% endfor %}
}

bool {{struct|unrealTypeName: false, moduleCapital}}::operator!=(const {{struct|unrealTypeName: false, moduleCapital}}& rhs) const
{
	return !operator==(rhs);
}

{% endfor %}
