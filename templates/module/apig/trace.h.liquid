#pragma once

#include "{{module.name|capital}}_apig.h"

{% for interface in module.interfaces %}
{% assign class = module.name | capital | append: interface.name | capital | append: "Tracer" %}

class {{class}}
{
public:
  {{class}}();
  static void capture_state(I{{module.name | capital}}{{interface.name|capital}}Interface* obj);
  {% for property in interface.properties %}
  static void trace_callSet{{property.name | capital}}({{property|unrealParam: '', 'In'}});
  {% endfor %}
  {% for signal in interface.signals %}
  static void trace_signal{{signal.name|capital}}({{signal | unrealParams}});
  {% endfor %}
  {% for operation in interface.operations %}
  static void trace_call{{operation.name|capital}}({{operation | unrealParams}});
  {% endfor %}
};
{% endfor %}
