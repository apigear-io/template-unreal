{% assign module_id = module.name|capital|append: "Editor" -%}
{% assign module_path = module.name|path -%}

namespace UnrealBuildTool.Rules
{
	public class {{module_id}} : ModuleRules
	{
		public {{module_id}}(ReadOnlyTargetRules Target) : base(Target)
		{
			PCHUsage = ModuleRules.PCHUsageMode.UseExplicitOrSharedPCHs;
			bEnforceIWYU = true;
			
			PrivateDependencyModuleNames.AddRange(
				new string[] {
					"Core",
					"CoreUObject",
					"UnrealEd",
				});

			PrivateIncludePaths.AddRange(
				new string[] {
					"{{module_id}}/Private",
				});		
				
			PrivateDependencyModuleNames.AddRange(
				new string[]
				{
					"{{module.name|capital}}"
				}
				);
			}
	}
}
